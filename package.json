{
  "name": "vscode-onetab",
  "displayName": "vscode-onetab",
  "description": "onetab for vscode",
  "author": {
    "name": "hsqStephenZhang",
    "email": "2250015961@qq.com"
  },
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.69.0"
  },
  "license": "MIT",
  "keywords": [
    "onetab",
    "vscode",
    "efficiency"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/hsqstephenZhang/vscode-onetab"
  },
  "bugs": {
    "url": "https://github.com/hsqstephenZhang/vscode-onetab/issues"
  },
  "extensionKind": [
    "workspace"
  ],
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "onetab.send.thisTab",
        "title": "%onetab.send.thisTab%"
      },
      {
        "command": "onetab.send.otherTabs",
        "title": "%onetab.send.otherTabs%"
      },
      {
        "command": "onetab.send.leftTabs",
        "title": "%onetab.send.leftTabs%"
      },
      {
        "command": "onetab.send.rightTabs",
        "title": "%onetab.send.rightTabs%"
      },
      {
        "command": "onetab.send.allTabs",
        "title": "%onetab.send.allTabs%"
      },
      {
        "command": "onetab.send.advanced.sendThisTab",
        "title": "%onetab.send.advanced.sendThisTab%"
      },
      {
        "command": "onetab.tab.restore",
        "title": "restore one tab",
        "icon": "$(refresh)"
      },
      {
        "command": "onetab.tab.remove",
        "title": "remove one tab",
        "icon": "$(close)"
      },
      {
        "command": "onetab.tabsGroup.restore",
        "title": "restore tabs",
        "icon": "$(refresh)"
      },
      {
        "command": "onetab.tabsGroup.rename",
        "title": "rename tabs group",
        "icon": "$(pencil)"
      },
      {
        "command": "onetab.tabsGroup.pin",
        "title": "pin tabs group",
        "icon": "$(check)"
      },
      {
        "command": "onetab.tabsGroup.remove",
        "title": "remove tabs group",
        "icon": "$(close)"
      },
      {
        "command": "onetab.advanced.webPage",
        "title": "onetab webpage"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "onetab",
          "title": "onetab",
          "icon": "media/dep.svg"
        }
      ]
    },
    "views": {
      "onetab": [
        {
          "id": "onetabs",
          "name": "onetabs",
          "icon": "media/dep.svg",
          "contextualTitle": "Tabs"
        },
        {
          "id": "feedback",
          "name": "feedback",
          "icon": "media/dep.svg",
          "contextualTitle": "FeedBack"
        }
      ]
    },
    "menus": {
      "editor/title/context": [
        {
          "group": "1_onetab",
          "submenu": "onetab.editor.title"
        }
      ],
      "editor/title": [
        {
          "group": "2_onetab",
          "submenu": "onetab.editor.title"
        }
      ],
      "view/item/context": [
        {
          "command": "onetab.tabsGroup.pin",
          "when": "view =~ /^(onetabs)$/ && viewItem =~ /^(tabsGroup)$/",
          "group": "inline@0"
        },
        {
          "command": "onetab.tabsGroup.rename",
          "when": "view =~ /^(onetabs)$/ && viewItem =~ /^(tabsGroup)$/",
          "group": "inline@1"
        },
        {
          "command": "onetab.tabsGroup.restore",
          "when": "view =~ /^(onetabs)$/ && viewItem =~ /^(tabsGroup)$/",
          "group": "inline@2"
        },
        {
          "command": "onetab.tabsGroup.remove",
          "when": "view =~ /^(onetabs)$/ && viewItem =~ /^(tabsGroup)$/",
          "group": "inline@3"
        },
        {
          "command": "onetab.tab.restore",
          "when": "view =~ /^(onetabs)$/ && viewItem =~ /^(tab)$/",
          "group": "inline@0"
        },
        {
          "command": "onetab.tab.remove",
          "when": "view =~ /^(onetabs)$/ && viewItem =~ /^(tab)$/",
          "group": "inline@1"
        }
      ],
      "onetab.editor.title": [
        {
          "command": "onetab.send.thisTab",
          "group": "onetab@0"
        },
        {
          "command": "onetab.send.otherTabs",
          "group": "onetab@1"
        },
        {
          "command": "onetab.send.leftTabs",
          "group": "onetab@2"
        },
        {
          "command": "onetab.send.rightTabs",
          "group": "onetab@3"
        },
        {
          "command": "onetab.send.allTabs",
          "group": "onetab@4"
        },
        {
          "command": "onetab.send.advanced.sendThisTab",
          "group": "onetab@5"
        }
      ]
    },
    "submenus": [
      {
        "id": "onetab.editor.title",
        "label": "%onetab.options%"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.69.0",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "@vscode/test-electron": "^2.1.5",
    "eslint": "^8.20.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "jshint": "^2.13.5"
  }
}